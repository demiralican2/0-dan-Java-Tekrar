public class JavaDeneme4 {
public static void main(String[] args) {


for(int i=0; i<=10; i++) {
			 for(int j=0; j<=i; j++) {
				 System.out.print((char)(j+65));
			 }
			 System.out.println();
		 }
		 for(int i=10; i>=0; i--) {
			 for(int j=0; j<=i; j++) {
				System.out.print((char)(j+65)); 
			 }
			 System.out.println();
		 }



}
}
